<?xml version='1.0' encoding='utf-8'?>
<widget id="co.araminco.muno" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>muno</name>
    <description>پخش رایگان موزیک با ترافیک رایگان !</description>
    <author email="info@muno.ir" href="http://mino.ir">
        muno.ir
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
